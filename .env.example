# LinkedIn API Credentials
LINKEDIN_API_KEY=your_linkedin_api_key_here
LINKEDIN_API_SECRET=your_linkedin_api_secret_here

# Glassdoor Credentials
GLASSDOOR_API_KEY=your_glassdoor_api_key_here

# GitHub API Token
GITHUB_TOKEN=your_github_token_here

# Stack Overflow API Key
STACKOVERFLOW_API_KEY=your_stackoverflow_key_here

# Email Configuration (for notifications and follow-ups)
EMAIL_ADDRESS=your_email@gmail.com
EMAIL_PASSWORD=your_app_password_here

# Database Configuration
DATABASE_PATH=./data/jobs.db

# Application Automation Settings
AUTO_APPLY_THRESHOLD=75
AUTO_APPLY_ENABLED=false

# Scheduler Settings (Cron format)
JOB_FETCH_SCHEDULE=0 6 * * *
JOB_MATCH_SCHEDULE=0 7 * * *

# Logging
LOG_LEVEL=INFO
LOG_FILE=./data/agent.log
